{
  "name": "bootstrap-vue-laravel-form",
  "description": "A form component built with bootstrap-vue that works with Laravel validation.",
  "version": "1.0.0",
  "author": "Incraigulous <me@craigwann.com>",
  "main": "src/Form.vue",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "jest"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "browser-env": "^3.2.4",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "jest": "^22.0.4",
    "moxios": "^0.4.0",
    "vue": "^2.5.11",
    "vue-jest": "^1.4.0",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "vue-test-utils": "^1.0.0-beta.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "peerDependencies": {
    "axios": "^0.17.0",
    "vue": "^2.4.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transformIgnorePatterns": [],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "mapCoverage": true
  }
}
